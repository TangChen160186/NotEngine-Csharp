                    xmlns:customControls="clr-namespace:Editor.Common.CustomControls">
   
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Controls/RenameableTextblock.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    <SolidColorBrush Color="#bbbbbb" x:Key="TreeviewItemTextBlockForeground"/>

    <SolidColorBrush Color="Black" x:Key="RenameableTextboxForeground"/>
